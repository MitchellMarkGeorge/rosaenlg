{
  "name": "rosaenlg-server-toolkit",
  "version": "0.3.2",
  "description": "Server common components for RosaeNLG",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "test": "nyc --reporter=lcov --reporter=text mocha",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:rosaenlg-projects/rosaenlg.git"
  },
  "keywords": [
    "RosaeNLG",
    "NLG",
    "server",
    "toolkit"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@rosaenlg.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/rosaenlg-projects/rosaenlg/issues"
  },
  "homepage": "https://rosaenlg.org",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.31",
    "@types/node-cache": "^4.2.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "aws-sdk": "^2.647.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "rosaenlg": "1.14.2",
    "s3rver": "^3.5.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "node-cache": "5.1.0",
    "rosaenlg-packager": "0.1.2"
  }
}

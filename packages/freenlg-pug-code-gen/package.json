{
  "name": "@freenlg/freenlg-pug-code-gen",
  "version": "2.6.0",
  "description": "Default code-generator for pug.  It generates HTML via a JavaScript template function. - FreeNLG fork",
  "scripts": {
    "build": "tsc && cp lib/index.js dist/",
    "test": "nyc mocha"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "main": "dist/index.js",
  "keywords": [
    "pug"
  ],
  "devDependencies": {
    "@types/node": "^9.6.6",
    "mocha": "^6.0.2",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.11",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@freenlg/french-verbs": "1.1.4",
    "@freenlg/german-adjectives": "1.2.0",
    "@freenlg/german-verbs": "1.1.3",
    "@freenlg/german-words": "1.1.3",
    "@freenlg/italian-adjectives": "1.1.0",
    "@freenlg/italian-words": "1.1.0",
    "constantinople": "^3.0.1",
    "debug": "^4.1.1",
    "doctypes": "^1.1.0",
    "js-stringify": "^1.0.1",
    "pug-attrs": "^2.0.3",
    "pug-error": "^1.3.2",
    "pug-runtime": "^2.0.4",
    "void-elements": "^2.0.1",
    "with": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:freenlg-projects/freenlg.git"
  },
  "author": "Ludan Stoeckl√©",
  "license": "MIT"
}

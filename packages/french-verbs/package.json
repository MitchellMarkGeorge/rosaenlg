{
  "name": "french-verbs",
  "version": "1.0.0",
  "description": "French verbs: conjugation, based on the Lefff contents, and list of transitive and intransitive verbs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha",
    "fromwiki": "node dist/create/createTransitive.js",
    "resources:build": "node dist/create/createList.js && copyfiles -f ./resources_src/etre.json ./resources_pub && copyfiles -f ./resources_src/intransitive.json ./resources_pub && copyfiles -f ./resources_src/transitive/transitive.json ./resources_pub/transitive",
    "resources:licence": "copyfiles -f ./resources_src/lefff-3.4.mlex/LICENSE ./resources_pub/conjugation && copyfiles -f ./resources_src/transitive/CC_BY-SA_3.0.html ./resources_pub/transitive",
    "preresources": "rimraf resources_pub/conjugation/* && rimraf resources_pub/transitive/* && rimraf resources_pub/*.json",
    "resources": "npm run resources:build && npm run resources:licence",
    "build": "tsc && npm run resources"
  },
  "dependencies": {
    "french-h-muet-aspire": "^1.0.0"
  },
  "devDependencies": {
    "copyfiles": "^2.0.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-sync-request": "^1.4.1"
  },
  "keywords": [
    "verbs",
    "conjugation",
    "French",
    "Lefff",
    "transitive",
    "intransitive",
    "auxiliary"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@gmail.com>",
  "license": "MIT"
}

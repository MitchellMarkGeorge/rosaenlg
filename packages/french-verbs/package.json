{
  "name": "french-verbs",
  "version": "2.5.5",
  "description": "French verbs: conjugation, based on the Lefff contents, and list of transitive and intransitive verbs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "nyc mocha",
    "fromwiki": "node dist/create/createTransitive.js",
    "resources:build": "node dist/create/createList.js && copyfiles -f ./resources_src/intransitive.json ./resources_pub && copyfiles -f ./resources_src/transitive/transitive.json ./resources_pub/transitive",
    "resources:licence": "copyfiles -f ./resources_src/lefff-3.4.mlex/LICENSE ./resources_pub/conjugation && copyfiles -f ./resources_src/transitive/CC_BY-SA_3.0.html ./resources_pub/transitive",
    "preresources": "rimraf resources_pub/conjugation/* && rimraf resources_pub/transitive/* && rimraf resources_pub/*.json",
    "resources": "npm run resources:build && npm run resources:licence",
    "build": "tsc && npm run resources"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:rosaenlg-projects/rosaenlg.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "dependencies": {
    "debug": "^4.1.1",
    "french-h-muet-aspire": "2.5.5"
  },
  "devDependencies": {
    "copyfiles": "^2.1.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "ts-sync-request": "^1.4.1",
    "typescript": "^3.7.2"
  },
  "keywords": [
    "verbs",
    "conjugation",
    "French",
    "Lefff",
    "transitive",
    "intransitive",
    "auxiliary"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@rosaenlg.org>",
  "license": "MIT"
}

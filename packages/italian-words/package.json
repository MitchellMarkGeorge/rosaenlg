{
  "name": "italian-words",
  "version": "2.0.2",
  "description": "Gender and plural of Italian words, based on morph-it",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "nyc mocha",
    "resources:build": "mkdir resources_pub && node dist/create/createList.js",
    "resources:licence": "copyfiles -f ./resources_src/morph-it/lgpl.txt ./resources_pub",
    "preresources": "rimraf resources_pub",
    "resources": "npm run resources:build && npm run resources:licence",
    "build:code": "tsc",
    "build": "npm run build:code && npm run resources"
  },
  "dependencies": {
    "debug": "^4.1.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "mocha": "^6.0.2",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.2"
  },
  "keywords": [
    "words",
    "gender",
    "plural",
    "Italian"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@rosaenlg.org>",
  "license": "MIT"
}

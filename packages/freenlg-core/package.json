{
  "name": "freenlg-core",
  "version": "0.15.6",
  "description": "core part of FreeNLG",
  "main": "dist/NlgLib.js",
  "scripts": {
    "grammars": "pegjs -o dist/french-grammar.js grammars/french-grammar.pegjs && pegjs -o dist/german-grammar.js grammars/german-grammar.pegjs",
    "build": "npm run grammars && tsc",
    "doc:html": "asciidoctor -D doc_output doc/*.adoc",
    "doc:pdf": "asciidoctor-pdf -D doc_output doc/*.adoc",
    "doc": "mkdir doc_output && cp doc/freenlg-logo.png doc_output && npm run doc:html && npm run doc:pdf",
    "publish:doc": "aws s3 cp doc_output/ s3://freenlg.org/ --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ludans/freenlg.git"
  },
  "keywords": [
    "NLG"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ludans/freenlg/issues"
  },
  "homepage": "https://github.com/ludans/freenlg",
  "devDependencies": {
    "@types/node": "^9.6.6",
    "junit": "^1.4.9",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "compromise": "^11.5.3",
    "moment": "^2.23.0",
    "pluralize-fr": "^1.0.1",
    "random-js": "^1.0.8",
    "written-number": "^0.8.1",
    "write-int": "^0.0.2",
    "numeral": "^2.0.6",
    "french-adjectives": "^1.0.0",
    "french-ordinals": "^1.0.0",
    "german-ordinals": "^1.0.0",
    "lefff-helper": "^1.0.0",
    "german-dict-helper": "^1.0.0",
    "freenlg-filter": "^1.0.0",
    "french-verbs": "^1.0.0",
    "french-words-gender": "^1.0.0",
    "german-words": "^1.0.0",
    "german-adjectives": "^1.0.0"
  }
}

{
  "name": "freenlg-core",
  "version": "0.14.0",
  "description": "core part of FreeNLG",
  "main": "index.js",
  "scripts": {
    "resources:build": "node resources_fr_FR_gender.js",
    "resources:licence": "copyfiles -f ./resources_src/fr_FR/lefff-3.4.mlex/LICENSE ./resources_pub/fr_FR",
    "preresources": "rimraf resources_pub/fr_FR/*",
    "resources": "npm run resources:build && npm run resources:licence",
    "build": "npm run resources",
    "doc:html": "asciidoctor -D doc_output doc/*.adoc",
    "doc:pdf": "asciidoctor-pdf -D doc_output doc/*.adoc",
    "doc": "mkdir doc_output && cp doc/freenlg-logo.png doc_output && npm run doc:html && npm run doc:pdf",
    "publish:doc": "aws s3 cp doc_output/ s3://freenlg.org/ --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ludans/freenlg.git"
  },
  "keywords": [
    "NLG"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ludans/freenlg/issues"
  },
  "homepage": "https://github.com/ludans/freenlg",
  "dependencies": {
    "compromise": "^11.5.3",
    "format-number-french": "^1.1.5",
    "jslingua": "^0.7.0",
    "pluralize-fr": "^1.0.1",
    "random-js": "^1.0.8",
    "titlecase": "^1.1.2",
    "titlecase-french": "^1.0.1"
  },
  "devDependencies": {
    "copyfiles": "^2.0.0",
    "junit": "^1.4.9",
    "rimraf": "^2.6.2"
  }
}

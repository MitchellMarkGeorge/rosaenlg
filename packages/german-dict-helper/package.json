{
  "name": "@freenlg/german-dict-helper",
  "version": "1.1.2",
  "description": "Helper functions on an included database derived from german-pos-dict",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "nyc mocha",
    "resources:build": "node dist/create/createDb.js",
    "resources:licence": "copyfiles -f ./resources_src/german-pos-dict/LICENSE ./resources_pub",
    "preresources": "rimraf resources_pub/*",
    "resources": "npm run resources:build && npm run resources:licence",
    "build": "tsc && npm run resources"
  },
  "dependencies": {
    "better-sqlite3": "^5.4.0",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "typescript": "^3.4.5",
    "copyfiles": "^2.1.0",
    "mocha": "^6.0.2",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3"
  },
  "keywords": [
    "German",
    "dictionnary",
    "NLP"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@freenlg.org>",
  "license": "MIT"
}

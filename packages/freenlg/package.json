{
  "name": "freenlg",
  "description": "A complete Natural Language Generation library based on Pug",
  "version": "0.15.6",
  "keywords": [
    "NLG",
    "Natural Language Generation"
  ],
  "author": "Ludan Stoeckle <ludan.stoeckle@gmail.com>",
  "maintainers": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:ludans/freenlg.git"
  },
  "bugs": {
    "url": "https://gitlab.com/ludan/freenlg/issues"
  },
  "homepage": "http://freenlg.org",
  "main": "dist/index.js",
  "scripts": {
    "grammars": "gulp grammars",
    "browserify": "gulp browserify",
    
    "build:before": "cp lib/index.js dist/",
    "build:main": "npm run grammars && tsc && gulp mainpug",
    "build:after": "npm run browserify",
    "build": " npm run build:before && npm run build:main && npm run build:after",
    
    "doc:html": "asciidoctor -D doc_output doc/*.adoc",
    "doc:pdf": "asciidoctor-pdf -D doc_output doc/*.adoc",
    "doc": "gulp doc && cp doc/freenlg-logo.png doc_output && npm run doc:html && npm run doc:pdf",
    "doc:publish": "gulp doc_s3",
    "test": "nyc mocha"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/",
      "examples",
      "doc",
      "lib",
      "register.js",
      "support"
    ]
  },
  "dependencies": {
    "compromise": "^11.13.1",
    "debug": "^4.1.1",
    "freenlg-filter": "^1.0.0",
    "freenlg-pug-code-gen": "2.4.5",
    "freenlg-pug-filters": "3.5.3",
    "freenlg-pug-lexer": "4.4.3",
    "freenlg-pug-linker": "3.4.3",
    "freenlg-pug-load": "2.4.3",
    "freenlg-pug-parser": "5.4.3",
    "freenlg-yseop": "1.0.0",
    "french-adjectives": "^1.0.0",
    "french-determiners": "^1.0.0",
    "german-determiners": "^1.0.0",
    "english-determiners": "^1.0.0",
    "french-ordinals": "^1.0.0",
    "french-verbs": "^1.0.0",
    "lefff-helper": "^1.0.0",
    "german-verbs": "^1.0.0",
    "french-words-gender": "^1.0.0",
    "german-adjectives": "^1.0.0",
    "german-ordinals": "^1.0.0",
    "german-words": "^1.0.0",
    "german-dict-helper": "^1.0.0",
    "synonym-optimizer": "^1.0.0",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "pluralize-fr": "^1.0.1",
    "pug-runtime": "^2.0.4",
    "pug-strip-comments": "^1.0.3",
    "random-js": "^2.0.0-rc3",
    "write-int": "^0.0.2",
    "written-number": "^0.9.1"
  },
  "devDependencies": {
    "@types/node": "^9.6.6",
    "jstransformer-cdata": "^1.0.0",
    "jstransformer-coffee-script": "^1.0.0",
    "jstransformer-less": "^2.1.0",
    "jstransformer-markdown-it": "^1.0.0",
    "jstransformer-stylus": "^1.0.0",
    "jstransformer-uglify-js": "^1.1.1",
    "jstransformer-verbatim": "^1.0.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "rimraf": "^2.2.8",
    "brfs": "^2.0.2",
    "browserify": "^16.2.3",
    "unassertify": "^2.1.1",
    "envify": "^4.1.0",
    "uglifyify": "^5.0.1",
    "common-shakeify": "^0.6.0",
    "browser-pack-flat": "^3.3.0",
    "uglify-js": "^3.5.4",
    "minify-stream": "^1.2.0",
    "typescript": "^3.3.3333",
    "source-map-support": "^0.5.11",
    "ts-node": "^8.0.3",
    "gulp": "^4.0.0",
    "gulp-pegjs": "^0.1.0",
    "gulp-concat": "^2.6.1",
    "browserify-versionify": "^1.0.6",
    "codemirror-minified": "^5.46.0",
    "vue-codemirror": "^4.0.6",
    "gulp-awspublish": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "merge-stream": "^1.0.1"
  },
  "browser": {
    "fs": false
  }
}

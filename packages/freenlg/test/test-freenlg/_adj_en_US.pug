
-
  util.expected = [
    '<t>',
      '<l>the old camera</l>',
      '<l>the white house</l>',
      '<l>a rich man</l>',
      '<l>this house</l>',
      '<l>this house</l>',
      '<l>that house</l>',

      '<l>white houses</l>',
      '<l>white houses</l>',
      '<l>these houses</l>',
      '<l>these houses</l>',
      '<l>those houses</l>',

    '</t>'
  ].join('');


t
  l the #[+agreeAdj('old', 'camera')] camera

  l #[+value('house', { det:'DEFINITE', adj:'white'})]
  l #[+value('man', { det:'INDEFINITE', adj:'rich'})]
  l #[+value('house', { det:'DEMONSTRATIVE'})]
  l #[+value('house', { det:'DEMONSTRATIVE', dist:'NEAR'})]
  l #[+value('house', { det:'DEMONSTRATIVE', dist:'FAR'})]

  l #[+value('house', { det:'DEFINITE', adj:'white', number:'P'})]
  l #[+value('house', { det:'INDEFINITE', adj:'white', number:'P'})]
  l #[+value('house', { det:'DEMONSTRATIVE', number:'P'})]
  l #[+value('house', { det:'DEMONSTRATIVE', number:'P', dist:'NEAR'})]
  l #[+value('house', { det:'DEMONSTRATIVE', number:'P', dist:'FAR'})]


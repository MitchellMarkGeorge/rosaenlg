
-
  util.expected = `
    der alten Gurke
    der alten Gurke
    der alten Gurke
    
    dieses alten Telefons
    dieses alten Telefons

    dieser neuen Gurke
    dieser neuen Gurke

    Ich liebe das neue Telefon
    Ich liebe das neue Telefon

    Ich liebe den dritten Mann

    die Äpfel
    die alten Äpfel
    der alten Äpfel
  `;


t
  l der #[+agreeAdj('alt', 'Gurke', {case:'GENITIVE', det:'DEFINITE'})] Gurke
  l #[+value('Gurke', {case:'GENITIVE', det:'DEFINITE', adj:'alt'})]
  l #[+value("<der alte Gurke>", {case:'GENITIVE'})]
  
  l dieses #[+agreeAdj('alt', 'Telefon', {case:'GENITIVE', det:'DEMONSTRATIVE'})] Telefons
  l #[+value("<dieses alt Telefon>", {case:'GENITIVE'})]

  l dieser #[+agreeAdj('neu', 'Gurke', {case:'GENITIVE', det:'DEMONSTRATIVE'})] Gurke
  l #[+value("<diese neu Gurke>", {case:'GENITIVE'})]

  //- lieben, hassen and verstehen take the Accusative
  l Ich liebe #[+value('Telefon', {case:'ACCUSATIVE', det:'DEFINITE', adj:'neu'})]
  l Ich liebe #[+value('<der neu Telefon>', {case:'ACCUSATIVE'})]

  //- dict goes to fünft included only
  l Ich liebe #[+value('Mann', {case:'ACCUSATIVE', det:'DEFINITE', adj:'dritt'})]

  l #[+value('<der Apfel P>')]
  l #[+value('<der alt Apfel P>')]
  l #[+value('<der alt Apfel P>', {case:'GENITIVE'})]

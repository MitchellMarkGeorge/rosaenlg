-
  var BIJOUX = {bla: 'bla'};
  setRefGenderNumber(BIJOUX, 'M', 'P');

t
  u
    l #[+substantive("bijou", BIJOUX)] : #[+agreeAdj('beau', BIJOUX)]
    l couleur : #[+agreeAdj('vert', 'couleur')]
    l poids : #[+agreeAdj('lourd', 'poids')]

  -
    var GENOU = {ge: 'nou'};
  u
    - setRefNumber(GENOU, 'P');
    l #[+substantive('genou', GENOU)]
    - setRefNumber(GENOU, 'S');
    l #[+substantive('genou', GENOU)]

  u
    - var subjP = getAnonymous('M', 'P');
    each subst in ["bijou", "oeil", "œil", "eau", "landau", "pneu", "sou", "gaz", "émail"]
      l #[+substantive(subst, subjP)]
  u
    each title in ["du vent dans les branches", "junior l'aventurier", "cet été à la plage"]
      l
        titlecase
          | #{title}
  //- misc
  u
    l bla de #[+value(9000)]
    l bla. de #[+value(9000)]


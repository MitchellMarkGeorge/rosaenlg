-
  util.expected = `
    blaâ€¦
    blabla
    blabla
    Some Uppercase

    singular
    plural
    a

    the toy looks
  `;


- var TOY = {};
- TOY.ref = 'toy_ref';
mixin toy_ref(obj, params)
  | the toy

t
  l bla #{'\u2026'}
  l bla #[+eatSpace()] bla
  l bla
    | #[+eatSpace()]
    |
    | bla

  l
    itemz {}
      item
        | #[+uppercaseWords('some uppercase')]

  - var PRODUCT = {pro:'duct'};
  l
    - setRefNumber(PRODUCT, 'S');
    | #{getSorP(['singular','plural'], PRODUCT)}
  l
    - setRefNumber(PRODUCT, 'P');
    | #{getSorP(['singular','plural'], PRODUCT)}
  l
    itemz {}
      item
        | #[+syn('a', 'b')]

  l #[+subjectVerb(TOY, 'look')]
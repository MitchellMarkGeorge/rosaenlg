-
  var BIJOUX = {bla: 'bla'};
  setRefNumber(BIJOUX, 'P');

t
  u
    l #[+substantive("bijou", BIJOUX)] : #[+agreeAdj('beau', BIJOUX)]
    l couleur : #[+agreeAdj('vert', 'couleur')]
    l poids : #[+agreeAdj('lourd', 'poids')]

  u
    - var subjP = getAnonymous('M', 'P');
    each subst in ["bijou", "oeil", "œil", "eau", "landau", "pneu", "sou", "gaz", "émail"]
      l #[+substantive(subst, subjP)]
  u
    l #[+value(562407)]
    l #[+value(4294967295)]
    l #[+value(562407, {'AS_IS': true})]
    l #[+value(5500, {'TEXTUAL':true })]
    l #[+value(2654564, {'TEXTUAL':true })]
    l #[+value(20, {'ORDINAL_TEXTUAL':true })]
    l #[+value(21, {'ORDINAL_TEXTUAL':true })]
    l #[+value(1, {'ORDINAL_NUMBER':true })]
    l #[+value(21, {'ORDINAL_NUMBER':true })]
    l #[+value(1562.407)]
    //- l #[+value(56000000, {prefix: "€", reduce: true})]

  u
    each title in ["du vent dans les branches", "junior l'aventurier", "cet été à la plage"]
      l
        titlecase
          | #{title}
  u
    - var d = new Date('1980-04-14');
    l #[+value(d)]
    l #[+value(d, 'LL')]
    l le #[+value(d, "dddd Do MMMM YYYY")]
    l le #[+value(new Date('2018-06-01'), "Do MMMM YYYY")]
    l #[+value( new Date(2018,01,1) , 'MMMM')]

-
  var subjS = getAnonymous('M', 'S');
  var subjP = getAnonymous('M', 'P');

t
  u
    l #[+agreeAdj('cool', subjP)]
    l bla:bla

  u
    l #[+substantive("tomato", subjS)]
    l #[+substantive("tomato", subjP)]
    l #[+substantive("addendum", subjP)]
    l #[+substantive("alga", subjP)]
    l #[+substantive("cranberry", subjP)]
    l #[+substantive("radius", subjP)]

  u
    l #{ util.compromise('London is calling').sentences().toNegative().out() }

  u
    l #[+value(562407)]
    l #[+value(562407, {'AS_IS': true})]
    l #[+value(5500, {'TEXTUAL':true })]
    l #[+value(1.3333, {'TEXTUAL':true })]
    l #[+value(2654564, {'TEXTUAL':true })]
    l #[+value(20, {'ORDINAL_NUMBER':true })]
    l #[+value(21, {'ORDINAL_NUMBER':true })]
    l #[+value(20, {'ORDINAL_TEXTUAL':true })]
    l #[+value(21, {'ORDINAL_TEXTUAL':true })]

  u
    l
      - var w = 'strange';
      titlecase
        | what is this
        if true
          |
          | #{w}
          |
        | thing?
  u
    - var d = new Date('1980-04-14');
    l #[+value(d, 'LL')]
    -
      var d2 = util.moment("2013W065").toDate(); // Basic (short) week, weekday
    l #[+value(d2, 'LL')]
    l #[+value( new Date(2018,01,1) , 'MMMM')]
    


include ../main

-
  util.expected = [
    '<t>',
      '<u>',
        '<l>cool</l>',
        '<l>bla: bla</l>',
      '</u>',

      '<u>',
        '<l>tomato</l>', 
        '<l>tomatoes</l>', 
        '<l>addenda</l>', 
        '<l>algae</l>',
        '<l>cranberries</l>',
        '<l>radii</l>',
      '</u>',

      '<u>',
        '<l>London is not calling</l>',
      '</u>',

      '<u>',
        '<l>562,407</l>',
        '<l>562407</l>',
        '<l>five thousand five hundred</l>',
        '<l>one point three three three three</l>',
        '<l>two million six hundred thousand fifty four thousand five hundred and sixty four</l>',
        '<l>20th</l>',
        '<l>21st</l>',
        '<l>twentieth</l>',
        '<l>twenty first</l>',

  util.expected = [
    '<t>',
      '<u>',
        '<l>il est</l>',
        '<l>ils sont</l>',
        '<l>ils oignent</l>',
        '<l>ils chantent</l>',
      '</u>',
      '<u>',
        '<l>il boira</l>',
        '<l>ils prendront</l>',
        '<l>ils firent</l>',
      '</u>',
      '<u>',
        '<l>il a bu</l>',
        '<l>ils ont été</l>',
        '<l>ils ont mangé</l>',
        '<l>il est allé</l>',
      '</u>',

    '</t>'
  ].join('');




-
  var subjS = getAnonymous('M', 'S');
  var subjP = getAnonymous('M', 'P');

t
  u
    l il #[+verb(subjS, 'être')]
    l ils #[+verb(subjP, 'être')]
    l ils #[+verb(subjP, 'oindre')]
    l ils #[+verb(subjP, 'chanter')]
  u
    l il #[+verb(subjS, {verb: 'boire', tense: 'FUTUR'})]
    l ils #[+verb(subjP, {verb: 'prendre', tense: 'FUTUR'})]
    l ils #[+verb(subjP, {verb: 'faire', tense: 'PASSE_SIMPLE'})]

  u
    l il #[+verb(subjS, {verb: 'boire', tense:'PASSE_COMPOSE', aux:'AVOIR'})]
    l ils #[+verb(subjP, {verb: 'être', tense:'PASSE_COMPOSE', aux:'AVOIR'})]
    l ils #[+verb(subjP, {verb: 'manger', tense:'PASSE_COMPOSE', aux:'AVOIR'})]
    l il #[+verb(subjS, {verb: 'aller', tense:'PASSE_COMPOSE', aux:'ETRE'})]


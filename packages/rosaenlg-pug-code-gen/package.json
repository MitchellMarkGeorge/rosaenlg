{
  "name": "rosaenlg-pug-code-gen",
  "version": "3.14.3",
  "description": "Default code-generator for pug. It generates HTML via a JavaScript template function. - RosaeNLG fork",
  "scripts": {
    "build": "sh ./fake_resources_before.sh && tsc && sh ./fake_resources_after.sh && cp lib/index.js dist/",
    "test": "nyc --reporter=lcov --reporter=text mocha"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "main": "dist/index.js",
  "keywords": [
    "pug"
  ],
  "devDependencies": {
    "@types/node": "^12.12.31",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.16",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "constantinople": "^4.0.1",
    "debug": "^4.1.1",
    "doctypes": "^1.1.0",
    "english-plurals": "0.6.3",
    "english-plurals-list": "0.6.3",
    "english-verbs-gerunds": "0.6.3",
    "english-verbs-helper": "0.6.3",
    "english-verbs-irregular": "0.6.3",
    "french-verbs": "2.14.3",
    "french-verbs-lefff": "0.7.3",
    "german-adjectives": "2.14.3",
    "german-adjectives-dict": "0.7.3",
    "german-verbs": "2.14.3",
    "german-verbs-dict": "0.7.3",
    "german-words": "2.14.3",
    "german-words-dict": "0.7.3",
    "italian-adjectives": "2.14.3",
    "italian-adjectives-dict": "0.7.3",
    "italian-verbs": "2.14.3",
    "italian-verbs-dict": "0.7.3",
    "italian-words": "2.14.3",
    "italian-words-dict": "0.7.3",
    "js-stringify": "^1.0.2",
    "pug-attrs": "^2.0.3",
    "pug-error": "^1.3.2",
    "pug-runtime": "^2.0.4",
    "recast": "^0.18.5",
    "void-elements": "^3.1.0",
    "with": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:rosaenlg-projects/rosaenlg.git"
  },
  "author": "Ludan Stoeckl√©",
  "license": "MIT"
}

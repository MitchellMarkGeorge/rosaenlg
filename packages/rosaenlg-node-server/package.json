{
  "name": "rosaenlg-node-server",
  "version": "1.14.2",
  "description": "Server for RosaeNLG, written in node.js",
  "main": "dist/server.js",
  "bin": {
    "rosaenlg-server": "./dist/server.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "cleanlogs": "echo {} > logs/combined.log",
    "test": "nyc mocha",
    "start": "node dist/server.js",
    "tsc": "tsc",
    "build:code": "tsc && gulp shebangify && chmod +x ./dist/server.js",
    "build:swagger": "gulp swagger && cd dist/ && redoc-cli bundle openApiDocumentation_merged.json",
    "build": "npm run build:code && npm run build:swagger"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:rosaenlg-projects/rosaenlg.git"
  },
  "keywords": [
    "RosaeNLG",
    "NLG",
    "server"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@rosaenlg.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/rosaenlg-projects/rosaenlg/issues"
  },
  "homepage": "https://rosaenlg.org",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.31",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "acorn": "^7.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-awspublish": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "json-refs": "^3.0.13",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "redoc-cli": "^0.9.7",
    "s3rver": "^3.5.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.2",
    "aws-sdk": "^2.647.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "rosaenlg": "1.14.2",
    "rosaenlg-packager": "0.1.2",
    "rosaenlg-server-toolkit": "0.3.2",
    "swagger-ui-express": "^4.1.2",
    "winston": "^3.2.1",
    "winston-aws-cloudwatch": "^3.0.0",
    "yn": "^4.0.0"
  }
}
